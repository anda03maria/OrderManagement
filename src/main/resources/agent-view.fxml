<Pane prefHeight="477.0" prefWidth="409.0" style="-fx-background-image: url('welcome.jpeg'); -fx-background-size: cover;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.order_management.ordermanagement.controller.AgentController">
    <TableView fx:id="productsTable" layoutX="14.0" layoutY="53.0" prefHeight="357.0" prefWidth="381.0">
